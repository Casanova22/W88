<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_app"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="vv.eighteight.w88conthoismash.FragmentWFirst"
        android:label="First Fragment"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_InfoFragment"
            app:destination="@id/InfoFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>

    <fragment
        android:id="@+id/SecondFragment"
        android:name="vv.eighteight.w88conthoismash.FragmentWSecond"
        android:label="Second Fragment"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@+id/FirstFragment"
            />
    </fragment>

    <fragment
        android:id="@+id/InfoFragment"
        android:name="vv.eighteight.w88conthoismash.FragmentWThird"
        android:label="Info Fragment"
        tools:layout="@layout/fragment_third">
        <action
            android:id="@+id/action_InfoFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>

</navigation>